{
  "name": "agentur1601com/PageTitleSuffix",
  "description": "Disable root title suffix and use only the page title",
  "type":"contao-module",
  "license":"LGPL-3.0+",
  "authors": [
    {
      "name"    : "Lukas Beck",
      "email"   : "lb@1601.com",
      "github"  : "LBeckX"
    }
  ],
  "require": {
    "php": ">=5.6",
    "contao/core-bundle":"^4.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.*-dev"
    },
    "contao-manager-plugin": "agentur1601com\\PageTitleSuffix\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "agentur1601com\\PageTitleSuffix\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/"
    ]
  }
}